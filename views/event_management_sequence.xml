<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data noupdate="1">
        <record id="sequence_ticket_payment_id" model="ir.sequence">
            <field name="name">Sequence Ticket Payment</field>
            <field name="code">ticket.payment</field>
            <field name="prefix">PAY/%(year)s/%(month)s</field>
            <field name="number_next">1</field>  <!-- Awal dari nomor berikutnya -->
            <field name="number_increment">1</field>  <!-- Increment nomor -->
            <field name="use_date_range">True</field>  <!-- Menggunakan rentang tanggal -->
            <field name="company_id" eval="False"/>  <!-- Jika tidak terkait dengan perusahaan -->
            <field name="padding">5</field>  <!-- Batas digit yang diatur -->
        </record>
    </data>
</odoo>
